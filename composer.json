{
    "name": "mushketer888/laravel-dblog",
    "description": "Extending Laravel Logger with Eqloquent Logging in DB",
    "keywords": [
        "laravel",
        "laravel-dblog",
        "dblog",
        "database",
        "log"
    ],
    "homepage": "https://github.com/mushketer888/laravel-dblog",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilja Lebedevs",
            "email": "mushketer888@gmail.com",
            "homepage": "https://github.com/mushketer888/laravel-dblog",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "phpunit/phpunit": "~5.7",
    },
    "autoload": {
        "psr-4": {
            "Mushketer888\\LaravelDblog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mushketer888\\LaravelDblog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "stable"
}
